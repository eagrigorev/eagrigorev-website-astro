{
	"version": 3,
	"routes": [
		{
			"src": "^/posts$",
			"headers": {
				"Location": "/"
			},
			"status": 301
		},
		{
			"src": "^/tags$",
			"headers": {
				"Location": "/"
			},
			"status": 301
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/.*$",
			"dest": "/404.html",
			"status": 404
		}
	]
}