---
interface Props {
  src: string;
}

const { src } = Astro.props;
---

<iframe
  title="Spotify music"
  style={{ borderRadius: '12px' }}
  src={src}
  width="100%"
  height="352"
  frameborder="0"
  allowfullscreen={false}
  allow="autoplay; clipboard-write; encrypted-media; fullscreen; picture-in-picture"
  loading="eager"></iframe>
