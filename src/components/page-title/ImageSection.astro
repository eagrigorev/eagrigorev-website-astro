---
import { Image } from 'astro:assets';
import pageTitleImage from '@images/assets/test.jpg';
---

<Image
  class="image"
  src={pageTitleImage}
  alt="PAGE TITLE IMAGE DESCRIPTION"
  width="1260"
  height="450"
  decoding="async"
  loading="eager"
  layout="constrained"
/>

<style lang="scss">
  .image {
    width: 100%;
    height: var(--space-250-450);
    aspect-ratio: auto;
  }
</style>
